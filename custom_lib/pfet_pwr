!!!!    2    0    1 1119957082  V8697                                         
test powered analog

test "ON"
test "OFF"

disconnect all
end test

subtest "ON"
   disconnect all
   connect s to pins "S"
   connect i to pins "S"
   connect l to pins "D"
   connect a to pins "G"
   
   source dcv,am3, terminated 500 , on
   auxiliary dcv, am-3, ico1, on
   detector dcv, expect 100m
   wait 10m
   measure 0.5, -0.1
end subtest

subtest "OFF"
   disconnect all
   connect s to pins "S"
   connect i to pins "S"
   connect l to pins "D"
   connect a to pins "G"
   
   source dcv,am3, terminated 500 , on
   auxiliary dcv, am3, ico1, on
   detector dcv, expect 3
   wait 10m
   measure 3.2, 2
end subtest

!
! End of test
!