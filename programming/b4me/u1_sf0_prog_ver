! Pause 10
 wait 10m
! SendByte 15 ! STORE_USER_ALL
 call WR_COM
 call WR(D_0,D_0,D_0,D_1,D_0,D_1,D_0,D_1)
 call STOP
! Pause 1000
 wait 1200m
 call WR_COM ! ReadByte_Mask 78 00 80 ! STATUS_BYTE
 call WR(D_0,D_1,D_1,D_1,D_1,D_0,D_0,D_0)
 call RD_COM
 call RN(R_0,R_X,R_X,R_X,R_X,R_X,R_X,R_X)
 call STOP
 call WR_COM ! ReadByte_Mask 80 00 03 ! STATUS_MFR_SPECIFIC
 call WR(D_1,D_0,D_0,D_0,D_0,D_0,D_0,D_0)
